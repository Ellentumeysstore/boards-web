.account-page-header {
  background-color: rgba(255, 255, 255, 0.98);
  left: 0;
  padding: 78px 15px 10px 250px;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 5;
}

[data-action="toggleSidebar"] {
  display: block;
  margin: 6px 0 0;
  padding-left: 0;

  .icon-bar {
    background: #D9D9D9;
  }

  .icon-sidebar-open {
    display: none;
  }

  .icon-sidebar-close {
    display: block;
  }
}

.content-header {
  margin-left: 35px;
}

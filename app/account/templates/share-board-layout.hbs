<div class="share-board-modal modal" id="share-board-modal" data-backdrop="static">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="ui-modal-alert"></div>
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>Sharing Settings</h3>
      </div> {{! .modal-header }}
      <div class="modal-body">
        <div class="form-group share-board-toggle"></div>
        <div class="form-group share-board-collaborators-wrapper">
          <strong class="share-board-collaborators-title">Collaborators</strong>
          <div class="share-board-collaborators"></div>
        </div> {{! .share-board-collaborators }}
        <div class="share-board-invite"></div>
      </div> {{! .modal-body }}
      <div class="modal-footer clearfix">
        <button type="button" class="btn btn-default pull-left" data-action="submitShareModal">Done</button>
      </div> {{! .modal-footer }}
    </div> {{! .modal-content }}
  </div> {{! .modal-dialog }}
</div> {{! #share-board-modal }}

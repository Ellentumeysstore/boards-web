<div class="board-collaborators-modal board-settings-modal modal" id="board-collaborators-modal" data-backdrop="static">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="ui-modal-alert"></div>
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>Collaborator Settings</h3>
      </div> {{! .modal-header }}
      <div class="modal-body">
        <div class="board-collaborators-wrapper">
          <strong class="board-settings-modal-title">People collaborating on this board</strong>
          <div class="board-collaborators-list"></div>
        </div> {{! .board-collaborators-wrapper }}
        <div class="board-collaborators-invite"></div>
      </div> {{! .modal-body }}
      <div class="modal-footer clearfix">
        <button type="button" class="btn btn-default pull-left" data-action="submitCollaboratorsModal">Done</button>
      </div> {{! .modal-footer }}
    </div> {{! .modal-content }}
  </div> {{! .modal-dialog }}
</div> {{! #board-collaborators-modal }}


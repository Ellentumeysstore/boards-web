{
  "name": "blimp-web",
  "version": "0.0.1",
  "main": "public/js/app.js",
  "dependencies": {
    "swag": "~0.5.1",
    "marked": "~0.3.1",
    "lodash": "~2",
    "jquery": "~2",
    "yepnope": "~1.5.4",
    "freewall": "git@github.com:kombai/freewall.git",
    "backbone": "~1",
    "bootstrap": "~3.1.1",
    "jquery-ui": "~1.10.4",
    "handlebars": "~1",
    "blueimp-md5": "~1.1.0",
    "jquery-timeago": "~1.4.0",
    "blueimp-file-upload": "~9.5.6"
  },
  "overrides": {
    "marked": {
      "main": "lib/marked.js"
    },
    "jquery": {
      "main": "dist/jquery.js"
    },
    "yepnope": {
      "main": "yepnope.js"
    },
    "backbone": {
      "main": "backbone.js",
      "dependencies": {
        "lodash": "*",
        "jquery": "*",
        "handlebars": "*"
      }
    },
    "freewall": {
      "main": "freewall.js"
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css"
      ]
    },
    "handlebars": {
      "main": "handlebars.runtime.js"
    },
    "blueimp-file-upload": {
      "dependencies": {
        "jquery": "*",
        "blueimp-load-image": "*",
        "blueimp-canvas-to-blob": "*"
      },
      "main": [
        "js/vendor/jquery.ui.widget.js",
        "js/jquery.iframe-transport.js",
        "js/jquery.fileupload.js",
        "js/jquery.fileupload-process.js",
        "js/jquery.fileupload-image.js",
        "js/jquery.fileupload-audio.js",
        "js/jquery.fileupload-video.js",
        "js/jquery.fileupload-validate.js",
        "js/jquery.fileupload-ui.js"
      ]
    }
